{
    "name": "Beta Resurgam",
    "description": "Learning all about Chrome Extensions Manifest Version 3!",
    "version": "0.1.0",
    "manifest_version": 3,
    "icons": {
        "32": "images/icon-32.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "home.html",
        "default_icon": {
            "32": "images/icon-32.png"
        }
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "tabs"
    ],
    "host_permissions": [
        "https://www.google.com/*"
    ]
}